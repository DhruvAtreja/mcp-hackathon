{
  "name": "skynet-mcp",
  "version": "1.0.0",
  "description": "SkyNet MCP Memory Implementation",
  "main": "dist/src/server/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/server/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server/index.ts",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e"
  },
  "keywords": [
    "mcp",
    "ai",
    "memory"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.4",
    "dotenv": "^16.5.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.3",
    "sequelize": "^6.37.7",
    "sqlite": "^4.0.0",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/sqlite3": "^3.1.7",
    "@types/uuid": "^8.3.4",
    "axios": "^1.9.0",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>/src/$1"
    }
  }
}
